<div class="starter-template">
  <h1>古本の検索結果</h1>
    <table class="table table-hover">
      <tr>
        <th>商品名</th>
        <th>値段</th>
      </tr>
      <% @rankings.each do |ranking| %>
      <tr>
        <td><%= ranking.name %></td>
        <td><%= ranking.price %></td>
      </tr>
      <% end %>
    </table>
</div>
